module yang-notification {
    yang-version 1.1;
    namespace "urn:opendaylight:tsdr:yang:collector:config:yang-notification";
    prefix "yang-notification";

    revision "2018-10-05" {
        description "Initial revision of yang notification model";
    }

    container notification-subscription {
        list notifications {

            key "notification";
            leaf notification {
               type string;
               config true;
               mandatory true;
            }
        }
    }
}
